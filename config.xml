<?xml version='1.0' encoding='utf-8'?>
<widget id="com.twilio.rtd.chat.js.cordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TwilioChatJsCordova</name>
    <description>
        A sample Apache Cordova application using Twilio Programmable Chat.
    </description>
    <author email="aivanovs@twilio.com" href="https://www.twilio.com">
        Twilio
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="google-services.json" target="google-services.json" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^7.0.0" />
    <engine name="ios" spec="^4.5.4" />
    <plugin name="phonegap-plugin-push" spec="~2.1.3" />
    <plugin name="cordova-plugin-twilio-chat" spec="~3.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" />
</widget>
